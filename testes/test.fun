fn main {
    # "println" is a builtin function to print the string to the console with
    # "fun3:" at the beginning.
    # print just prints the text as is, for regular printing without a newline
    # at the end use "print"
    
    println "Hello, world!";
    println [add 1 2];
    
    print "------";

    print "Hello, world!";
    print [add 1 2];
}
